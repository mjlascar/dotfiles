{
    "hyprland/workspaces": {
        "format": "{icon}",
        "format-icons": {
            "1": "I",
            "2": "II",
            "3": "III",
            "4": "IV",
            "5": "V",
            "6": "VI",
            "7": "VII",
            "8": "VIII",
            "9": "IX",
            "10": "X"
        },
        "on-click": "activate",
        "sort-by-number": true,
        "active-only": false
    },
    "hyprland/window": {
        "format": "<span foreground=\"#A7C080\"> </span> {initialTitle}",
        "max-length": 100
    },
    "clock": {
        "timezone": "America/Argentina/Buenos_Aires",
        "format-alt": "{:%H:%M}",
        "format": "{:%a, %d. %b %Y - %H:%M}"
    },
    "tray": {
        "icon-size": 21,
        "spacing": 10,
        "show-passive-items": true
    },
    "backlight": {
        "device": "intel_backlight",
        "format": "<span foreground=\"#A7C080\"> {icon} </span><span weight=\"bold\"> {percent}% </span>",
        "format-icons": [
            "",
            ""
        ],
        "on-click": "pkill -USR1 gammastep || gammastep",
        "tooltip-format": "Brightness"
    },
    "pulseaudio": {
        "format": "<span foreground=\"#3D484D\">|</span> <span foreground=\"#A7C080\">{icon} </span><span weight=\"bold\"> {volume}% </span><span foreground=\"#3D484D\">|</span>",
        "format-bluetooth": "<span foreground=\"#3D484D\">|</span> <span foreground=\"#A7C080\"></span><span weight=\"bold\"> {volume}% </span>",
        "format-muted": "<span foreground=\"#3D484D\">|</span> <span foreground=\"#A7C080\"> </span><span weight=\"bold\">X </span><span foreground=\"#3D484D\">|</span>",
        "format-icons": {
            "headphone": "",
            "default": [
                "",
                ""
            ]
        },
        "scroll-step": 1,
        "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
        "on-click-right": "pavucontrol",
        "ignored-sinks": [
            "Easy Effects Sink"
        ]
    },
    "network": {
        "format": "   ",
        "format-wifi": "<span foreground=\"#A7C080\">   </span>",
        "format-ethernet": "<span foreground=\"#A7C080\"> 󰊗 </span>",
        "format-disconnected": "   ",
        "on-click": "kitty -e nmtui",
        "tooltip-format": "{ifname} via {gwaddr}",
        "tooltip-format-wifi": "{essid} ({signalStrength}%)",
        "tooltip-format-ethernet": "{ifname\n{ipaddr}/{cidr}}",
        "tooltip-format-disconnected": "zzz"
    },
    "bluetooth": {
        "format-on": "<span foreground=\"#3D484D\">|</span><span foreground=\"#A7C080\" weight=\"bold\">  </span>",
        "format-connected": "<span foreground=\"#3D484D\">|</span><span foreground=\"#A7C080\" weight=\"bold\">  </span>",
        "format": "<span foreground=\"#3D484D\">|</span><span>  </span>",
        "on-click": "rfkill toggle bluetooth",
        "on-click-right": "blueman-manager",
        "tooltip-format": "zzz",
        "tooltip-format-on": "prendido perro",
        "tooltip-format-connected": "{device_enumerate}",
        "tooltip-format-enumerate-connected": "{device_alias}"
    },
    "hyprland/language": {
        "format": "<span foreground=\"#A7C080\"> 󰌌 </span> <span foreground=\"#3D484D\">|</span>",
        "tooltip-format": " {short} "
    },
    "battery": {
        "states": {
            "good": 90,
            "warning": 30,
            "critical": 20
        },
        "format": "<span color=\"#A7C080\"> {icon}</span><span weight=\"bold\"> {capacity}% </span>",
        "format-charging": "<span foreground=\"#A7C080\"> {icon}  </span><span weight=\"bold\">{capacity}% </span>",
        "format-full": "<span foreground=\"#82A55F\"> {icon} </span>",
        "format-icons": [
            "󰁻",
            "󰁼",
            "󰁾",
            "󰂀",
            "󰂂",
            "󰁹"
        ],
        "tooltip-format": "{timeTo} | {capacity} % | {power} W"
    },
    "temperature": {
        "format": "<span color=\"#A7C080\"> </span><span weight=\"bold\">{temperatureC}°C </span>",
        "format-critical": "<span color=\"#E42022\"> </span><span color=\"#E42022\" weight=\"bold\">{temperatureC}°C </span>",
        "hwmon-path": "/sys/class/hwmon/hwmon3/temp1_input",
        "critical-threshold": 50,
        "tooltip": false
    },
    "custom/cputemp": {
        "format": "{}",
        "exec": "~/.config/waybar/bin/cputemp",
        "interval": 10,
        "return-type": "json"
    },
    "cpu": {
        "format": " <span foreground=\"#A7C080\" weight=\"bold\">CPU</span><span weight=\"bold\"> {usage}% </span>"
    },
    "memory": {
        "format": "<span foreground=\"#3D484D\">|</span> <span foreground=\"#A7C080\" weight=\"bold\">RAM</span><span weight=\"bold\"> {}% </span>",
        "on-click": "kitty -e htop"
    },
    "custom/wlogout": {
        "format": "⏻",
        "on-click": "wlogout",
        "tooltip": false
    }
}
